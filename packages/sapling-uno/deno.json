{
  "name": "@sapling/sapling-uno",
  "description": "A micro SSR framework with UnoCSS support",
  "license": "MIT",
  "version": "0.8.0",
  "tasks": {
    "dev": "deno run --watch main.ts"
  },
  "imports": {
    "@hono/hono": "jsr:@hono/hono@^4.8.12",
    "@std/assert": "jsr:@std/assert@1",
    "@unocss/core": "npm:@unocss/core@^66.4.0",
    "@unocss/preset-wind3": "npm:@unocss/preset-wind3@^66.4.0"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "@hono/hono/jsx",
    "lib": ["ESNext", "DOM", "DOM.Iterable", "deno.ns"]
  },
  "nodeModulesDir": "auto"
}
