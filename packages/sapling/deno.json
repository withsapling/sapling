{
  "name": "@sapling/sapling",
  "description": "A micro SSR framework",
  "license": "MIT",
  "version": "0.7.1",
  "imports": {
    "@hono/hono": "jsr:@hono/hono@^4.7.5",
    "@std/media-types": "jsr:@std/media-types@^1.1.0",
    "@std/path": "jsr:@std/path@^1.0.8",
    "@unocss/core": "npm:@unocss/core@^66.1.3",
    "@unocss/preset-wind3": "npm:@unocss/preset-wind3@^66.1.3",
    "urlpattern-polyfill": "npm:urlpattern-polyfill@^10.0.0"
  },
  "exports": {
    ".": "./src/index.ts"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "@hono/hono/jsx",
